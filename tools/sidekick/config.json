{
  "project": "Beervana Pop",
  "plugins": [
    {
      "id": "library",
      "title": "Block Library",
      "environments": ["edit"],
      "url": "/tools/sidekick/library.html?path=/tools/sidekick/blocks/authoring-guide&index=0",
      "includePaths": ["**.docx**"]
    },
    {
      "id": "asset-library",
      "title": "Assets",
      "environments": ["edit"],
      "url": "https://experience.adobe.com/solutions/CQ-assets-selectors/static-assets/resources/franklin/asset-selector.html",
      "isPalette": true,
      "includePaths": ["**.docx**"],
      "paletteRect": "top: 50px; bottom: 10px; right: 10px; left: auto; width:400px; height: calc(100vh - 60px)"
    },
    {
      "id": "link-validator",
      "title": "Link Validator",
      "environments": ["any"],
      "url": "/tools/sidekick/link-validator.html",
      "includePaths": ["**.docx**"],
      "passConfig": true,
      "passReferrer": true,
      "isPalette": true,
      "paletteRect": "top: 50px; bottom: 10px; right: 10px; left: auto; width:400px; height: calc(100vh - 60px)"
    }
  ]
}
